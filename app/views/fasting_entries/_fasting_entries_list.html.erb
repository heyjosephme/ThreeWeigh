<% if fasting_entries.any? %>
  <ul class="divide-y divide-gray-200">
    <% fasting_entries.each do |fasting_entry| %>
      <%= render 'fasting_entry_row', fasting_entry: fasting_entry %>
    <% end %>
  </ul>
<% else %>
  <div class="text-center py-12">
    <svg class="mx-auto h-12 w-12 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
    </svg>
    <h3 class="mt-2 text-sm font-medium text-gray-900">No fasting entries</h3>
    <p class="mt-1 text-sm text-gray-500">Get started with your first fast above.</p>
  </div>
<% end %>